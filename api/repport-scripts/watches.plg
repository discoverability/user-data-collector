reset
set xtics rotate by 45 right
set terminal png size 2048,768;
set output '../app/static/img/repport/watches.png'
set datafile separator " "
set format x "%d/%b"
set timefmt "%d/%b/%Y"
set xdata time

f(x) = a*x + b
fit f(x) 'watches.csv' using 2:1 via a, b


plot "watches.csv" using 2:1 with line linewidth 3 smooth unique title "watches", f(x) title "linear fit"
